{
  "name": "angular-orz-image-filter",
  "version": "0.0.0",
  "description": "An img helper directive for some cross-platform effects, like: blur",
  "license": "MIT",
  "repository": "",
  "author": "Juntao Wu",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "preinstall": "jspm install",
    "clean": "rimraf lib && rimraf coverage && rimraf .nyc_output && rimraf lib_test",
    "prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublish": "npm run build",
    "prebuild": "npm run clean && npm run prettier && npm run lint && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty && node build",
    "test": "npm run clean && npm run build && tsc -p tsconfig.test.json --pretty && karma start & karma run",
    "coverage": "nyc report --reporter=lcov --reporter=text --reporter=html",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/angular": "^1.6.25",
    "@types/chai": "^4.0.1",
    "@types/node": "^7.0.0",
    "@types/should": "^8.3.0",
    "ava": "^0.20.0",
    "ava-jspm-loader": "^1.0.4",
    "chai": "^4.0.2",
    "coveralls": "^2.0.0",
    "karma": "^1.7.0",
    "karma-jspm": "^2.2.3",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.4.2",
    "mocha-typescript": "^1.1.7",
    "nyc": "^10.0.0",
    "prettier": "^1.5.2",
    "rimraf": "^2.0.0",
    "serve": "^6.0.1",
    "should": "^11.2.1",
    "systemjs-builder": "^0.16.9",
    "tslint": "^5.0.0",
    "tslint-config-prettier": "^1.1.0",
    "typescript": "^2.4.1"
  },
  "peerDependencies": {
    "angular": "1.6.5"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "ava": {
    "require": [
      "babel-register",
      "ava-jspm-loader"
    ]
  },
  "jspm": {
    "configFile": "system.config.js",
    "dependencies": {},
    "devDependencies": {
      "angular": "npm:angular@^1.6.5",
      "babel": "npm:babel-core@^5.6.4",
      "babel-runtime": "npm:babel-runtime@^5.6.4",
      "chai": "npm:chai@^4.0.2",
      "core-js": "npm:core-js@^0.9.4",
      "mocha": "npm:mocha@^3.4.2",
      "mocha-typescript": "npm:mocha-typescript@^1.1.7",
      "typescript": "npm:typescript@^2.4.1"
    }
  }
